{
  "name": "edgar/ez-uitreemenu-bundle",
  "license": "GPL-2.0-only",
  "type": "ezplatform-bundle",
  "description": "eZ Platform UI Tree content browse menu bundle",
  "autoload": {
    "psr-4": {
      "Edgar\\EzUITreeMenuBundle\\": "src/bundle/",
      "Edgar\\EzUITreeMenu\\": "src/lib/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Edgar\\EzUITreeMenuBundle\\Tests\\": "src/bundle/Tests" }
  },
  "require": {
    "php": ">=7.1",
    "ezsystems/ezpublish-kernel": "^7.1",
    "ezsystems/ezplatform-admin-ui": "^1.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.7.1",
    "phpunit/phpunit": "^6.4"
  },
  "scripts": {
    "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
  }
}
